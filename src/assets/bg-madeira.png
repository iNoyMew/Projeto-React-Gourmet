import { useSelector, useDispatch } from "react-redux";
import { removeCard, clearCard } from "./cardSlice";
import { Button } from "reactstrap";
import CardItem from "./CardItem";
import "@fontsource/poppins"; // Importa a fonte Poppins

const CardList = () =&gt; {
    const cards = useSelector(state =&gt; state.card.cards || []);
    const dispatch = useDispatch();

    return (
        &lt;div className="bg-white shadow-lg rounded-lg p-8 font-poppins"&gt;
            &lt;h2 className="text-4xl font-bold text-gray-800 text-center mb-8 tracking-wide"&gt;📜 Nosso Cardápio&lt;/h2&gt;

            {cards.length === 0 ? (
                &lt;p className="text-center text-gray-500 text-lg"&gt;Nenhum prato cadastrado.&lt;/p&gt;
            ) : (
                &lt;div className="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-8"&gt;
                    {cards.map((item) =&gt; (
                        &lt;div key={item.id} className="bg-gray-50 rounded-xl shadow-md p-6 transform transition duration-300 hover:scale-105 hover:shadow-xl"&gt;
                            &lt;CardItem item={item} /&gt;
                            &lt;div className="flex justify-between mt-5"&gt;
                                &lt;Button
                                    onClick={() =&gt; dispatch(removeCard(item.id))}
                                    color="danger"
                                    className="px-5 py-2 text-white bg-red-500 rounded-lg hover:bg-red-600 transition duration-300 font-medium"
                                &gt;
                                    Remover
                                &lt;/Button&gt;
                            &lt;/div&gt;
                        &lt;/div&gt;
                    ))}
                &lt;/div&gt;
            )}

            {cards.length &gt; 0 &amp;&amp; (
                &lt;div className="text-center mt-8"&gt;
                    &lt;Button
                        onClick={() =&gt; dispatch(clearCard())}
                        color="success"
                        className="px-8 py-3 text-white bg-green-500 rounded-lg hover:bg-green-600 transition duration-300 text-lg font-semibold"
                    &gt;
                        Limpar Todos os Pratos
                    &lt;/Button&gt;
                &lt;/div&gt;
            )}
        &lt;/div&gt;
